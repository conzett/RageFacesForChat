{
  "name": "Rage Faces for Chat",
  "version": "0.1",
  "description": "FFFFFFFUUUUUUUUUUUU",
  "icons": { "16": "icons/icon16.png",
             "48": "icons/icon48.png",
            "128": "icons/icon128.png" },
  "default_locale": "en",
  "permissions": ["https://mail.google.com/*", "http*://*.facebook.com/*"],

  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "css":["css/style.css"],
      "js": ["lib/jquery-1.7.1.min.js", "src/common.js", "src/gchat.js"]
    },
    {
      "matches": ["http://*.facebook.com/*", "https://*.facebook.com/*"],
      "css":["css/style.css"],
      "js": ["lib/jquery-1.7.1.min.js", "src/common.js", "src/facebook.js"]
    }
  ]
}